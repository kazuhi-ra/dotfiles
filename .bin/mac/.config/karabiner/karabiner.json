        "rules": [
          {
            "description": "次の単語の末尾に移動",
            "manipulators": [
              {
                "from": {
                  "key_code": "f",
                  "modifiers": {
                    "mandatory": ["control", "command"],
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "right_arrow",
                    "modifiers": ["option"]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "前の単語の先頭に移動",
            "manipulators": [
              {
                "from": {
                  "key_code": "m",
                  "modifiers": {
                    "mandatory": ["control", "command"],
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "left_arrow",
                    "modifiers": ["option"]
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "→",
            "manipulators": [
              {
                "from": {
                  "key_code": "f",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "right_arrow"
                  }
