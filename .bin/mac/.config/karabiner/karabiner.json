            "manipulators": [
              {
                "from": {
                  "key_code": "n",
                  "modifiers": {
                    "mandatory": ["control"],
                    "optional": ["any"]
                  }
                },
                "to": [
                  {
                    "key_code": "down_arrow"
                  }
                ],
