                  {
                    "key_code": "japanese_eisuu"
                  }
                ],
                "type": "basic"
              },
              {
                "conditions": [
                  {
                    "keyboard_types": ["jis"],
                    "type": "keyboard_type_if"
                  }
                ],
                "from": {
                  "key_code": "close_bracket",
                  "modifiers": {
                    "mandatory": ["control"]
                  }
                },
                "to": [
                  {
                    "key_code": "escape"
                  },
                  {
                    "key_code": "japanese_eisuu"
                  }
                ],
                "type": "basic"
              }
            ]
          },
